{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "MediaAppBucketAccess",
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::media-app-initial-image",
                "arn:aws:s3:::media-app-initial-image/*",
                "arn:aws:s3:::media-app-resized-image",
                "arn:aws:s3:::media-app-resized-image/*"
            ]
        }
    ]
}
